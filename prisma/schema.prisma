generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Course {
  id      Int     @id @default(autoincrement())
  name    String
  credits Int
  status  Boolean
}

model User {
    id      Int     @id @default(autoincrement())
    userName String @unique
    password String
    DOB DateTime?
    gender String?
    accessToken  String?  
    refreshToken String?  
    
}